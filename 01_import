USING PERIODIC COMMIT
LOAD CSV WITH HEADERS FROM "file:/metav/CSV/Books.csv" AS row
CREATE (b:Book)
set b = row

USING PERIODIC COMMIT
LOAD CSV WITH HEADERS FROM "file:/metav/CSV/People.csv" AS row
CREATE (p:Person)
set p = row

USING PERIODIC COMMIT
LOAD CSV WITH HEADERS FROM "file:/metav/CSV/Places.csv" AS row
CREATE (p:Place)
set p = row

